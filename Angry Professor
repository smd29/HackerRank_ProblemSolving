#include <assert.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int t,i,j,n,k,ar[1000];
    scanf("%d",&t);
    for(i=0;i<t;i++)
    {
        scanf("%d %d",&n,&k);
        for(j=0;j<n;j++)
       {
        scanf("%d",&ar[j]);
        if(ar[j]<=0)
            k--;;   
        }
        if(k<=0)
        { printf("NO\n");}
        else {
            printf("YES\n");}
    }
   
    return 0;
}
