#include <assert.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int m,x1,v1,x2,v2;
    scanf("%d %d %d %d",&x1,&v1,&x2,&v2);
  //double t=(x2-x1)/(v1-v2);
    //            m=round(t);
    if(x1<x2)
    {
        if(v1<=v2)
            printf("NO");
        else
            {
                if((x2-x1)%(v1-v2)==0)
                    printf("YES");
                else
                    printf("NO");
        }
    }
    else if(x1=x2)
    {
        if(v1==v2)
            printf("YES");
        else 
            printf("NO");
    }
    else{
        if(v1>=v2)
            printf("NO");
        else
        {
                 if((x2-x1)%(v1-v2)==0)
                    printf("YES");
                else
                    printf("NO");
        }
    }
    return 0;
}
